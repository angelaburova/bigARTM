# -*- coding: utf-8 -*-
import re
def sumstr(arr):
    ss = ''
    for j in len(arr):
        if j % 2 == 0:
            ss += arr[j]+'_'
        else:
            ss += arr[j]+' '
    return ss



f = open('autors.txt', 'r')
lines = f.readlines()
f.close()
new_lines = []
autors = []
s = ''
f2 = open('autors2.0.txt', 'a')
for i in new_lines:
    autors = i.split(' ')
    str = sumStr(autors)
    f2.write(str + '\n')

